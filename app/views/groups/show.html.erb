<h1><%= @group.name %></h1>
<h3>Members of the group</h3>
<% @group.users.each do |user| %>
  <%= user[:username].capitalize %><br>
<% end %>

<div>
  <%= link_to "Start the quiz", new_group_quizz_choice_path(@group) %>
</div>
<div>
  <%= link_to "Delete this group", group_path(@group.id), method: "patch" %>
</div>
