<div class="progress my-5">
  <div class="progress-bar" role="progressbar" style="width: 0%;" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">
    0%
  </div>
</div>

<h3 class="my-5">Any wish ?</h3>

<%= form_tag(add_keyword_quizz_choice_path(@quizz_choice), method: 'patch', class: "form-inline") do %>
  <div class="d-flex justify-content-center w-100 fs-1">
    <div class="form-search search-data" data-autocomplete-choices-value="<%= @keywords_list.flatten.to_json %> " data-controller="autocomplete">
      <%= text_field_tag :q, nil, autocomplete: :off, class: "form-control search-input", placeholder: "Search a keyword...", data: { "autocomplete-target": "searchInput" } %>
    </div>
    <%= submit_tag "Next", class: "bottom-button" %>
  </div>
<% end %>
