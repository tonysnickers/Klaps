<div class="container">
<br>
  <div class="progress">
    <div class="progress-bar" role="progressbar" style="width: 75%;" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100">75%</div>
  </div>
<br>

<h3>Is there an actor you would like to see?</h3>
<br>
  <%= form_tag(add_actor_quizz_choice_path(@quizz_choice), method: 'patch', class: "form-inline") do %>
    <div id= "search-data" class="form-search search-data" data-skills="<%= @actor_list.to_json %>">
      <%= text_field_tag :q, nil, autocomplete: :off, class: "form-control search-input", placeholder: "Search an actor..." %>
      <br>
      <%= submit_tag "Search", class: "btn btn-primary button-home" %>
    </div>
  <% end %>

</div>
